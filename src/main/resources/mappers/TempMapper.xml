<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moelyon.learning.mybatis.dao.TempMapper">
    
    <select id="findById" resultType="com.moelyon.learning.mybatis.entity.TempEntity">
        select id,name  from temp
        where  id = #{id}
    </select>
    <select id="findAll" resultType="com.moelyon.learning.mybatis.entity.TempEntity">
        select id,name from temp;
    </select>
</mapper>